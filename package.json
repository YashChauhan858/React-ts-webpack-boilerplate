{
  "name": "react-ts-webpack-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate for webpack, react and typescript from scratch ",
  "main": "index.tsx",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dashboard -- webpack serve --config configuration/webpack/webpack.dev.js",
